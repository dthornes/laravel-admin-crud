{
    "name": "laravelcrudgenerator/laravel-crud-generator",
    "description": "",
    "keywords": [
        "laravelcrudgenerator",
        "laravel-crud-generator"
    ],
    "homepage": "https://github.com/dthornes/laravel-crud-generator",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dan Thornes",
            "email": "daniel.thornes@drpgroup.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelCrudGenerator\\LaravelCrudGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelCrudGenerator\\LaravelCrudGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelCrudGenerator\\LaravelCrudGenerator\\LaravelCrudGeneratorServiceProvider"
            ],
            "aliases": {
                "LaravelCrudGenerator": "LaravelCrudGenerator\\LaravelCrudGenerator\\LaravelCrudGeneratorFacade"
            }
        }
    }
}
